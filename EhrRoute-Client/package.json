{
   "name": "ehr-route-client",
   "version": "0.0.0",
   "main": "main.js",
   "scripts": {
      "ng": "ng",
      "start": "ng serve",
      "build": "ng build",
      "test": "ng test",
      "lint": "ng lint",
      "e2e": "ng e2e",
      "electron-run": "electron .",
      "electron-build-aot": "ng build --prod && electron .",
      "electron-build": "npm run postinstall:electron && ng build",
      "electron-start": "npm run postinstall:electron && npm-run-all -p start electron-run",
      "postinstall": "npm run postinstall:electron && npx electron-builder install-app-deps",
      "postinstall:electron": "node postinstall",
      "pack": "electron-builder --dir",
      "dist": "electron-builder",
      "electron-pack": "electron-packager ."
   },
   "private": true,
   "dependencies": {
      "@angular/animations": "~7.0.0",
      "@angular/common": "~7.0.0",
      "@angular/compiler": "~7.0.0",
      "@angular/core": "~7.0.0",
      "@angular/forms": "~7.0.0",
      "@angular/http": "~7.0.0",
      "@angular/platform-browser": "~7.0.0",
      "@angular/platform-browser-dynamic": "~7.0.0",
      "@angular/router": "~7.0.0",
      "@clr/icons": "^1.0.0",
      "@clr/ui": "^1.0.0",
      "@fortawesome/fontawesome-free": "^5.6.3",
      "@ng-bootstrap/ng-bootstrap": "^4.0.0",
      "@types/es6-shim": "^0.31.39",
      "@webcomponents/custom-elements": "^1.0.0",
      "bootstrap": "^4.3.1",
      "core-js": "^2.5.4",
      "crypto-js": "^3.1.9-1",
      "electron-dl": "^1.13.0",
      "electron-reload": "^1.4.0",
      "hammerjs": "^2.0.8",
      "jquery": "^3.3.1",
      "linvodb3": "^3.26.0",
      "merkletreejs": "0.0.22",
      "ng-click-outside": "^4.0.0",
      "ng-event-source": "^1.0.14",
      "ng-zorro-antd": "^7.0.0-rc.1",
      "ngx-electron": "^2.0.0",
      "popper.js": "^1.14.7",
      "reflect-metadata": "^0.1.12",
      "rxjs": "~6.3.3",
      "sqlite3": "^4.0.4",
      "typeorm": "^0.2.11",
      "wait-on": "^3.2.0",
      "zone.js": "~0.8.26"
   },
   "devDependencies": {
      "@angular-devkit/build-angular": "^0.12.1",
      "@angular/cli": "~7.0.3",
      "@angular/compiler-cli": "^7.2.9",
      "@angular/language-service": "~7.0.0",
      "@types/jasmine": "~2.8.8",
      "@types/jasminewd2": "~2.0.3",
      "@types/node": "^8.9.5",
      "codelyzer": "~4.5.0",
      "electron": "^3.0.13",
      "electron-builder": "^20.38.5",
      "electron-packager": "^13.0.1",
      "electron-rebuild": "^1.8.2",
      "jasmine-core": "~2.99.1",
      "jasmine-spec-reporter": "~4.2.1",
      "karma": "~3.0.0",
      "karma-chrome-launcher": "~2.2.0",
      "karma-coverage-istanbul-reporter": "~2.0.1",
      "karma-jasmine": "~1.1.2",
      "karma-jasmine-html-reporter": "^0.2.2",
      "npm-run-all": "^4.1.5",
      "protractor": "~5.4.0",
      "ts-node": "~7.0.0",
      "tslint": "~5.11.0",
      "typescript": "~3.1.1"
   },
   "build": {
      "appId": "edu.project.ehroute",
      "productName": "EHRoute",
      "files": [
         "**/*",
         "dist/**/*"
      ],
      "directories": {
         "output": "release",
         "buildResources": "dist"
      },
      "asar": false,
      "win": {
         "target": "nsis",
         "icon": "build/icon.ico"
      },
      "nsis": {
         "runAfterFinish": true,
         "license": "LICENSE"
      }
   }
}
